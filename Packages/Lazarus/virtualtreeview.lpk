<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="4">
    <Name Value="virtualtreeview"/>
    <Author Value="Mike Lischke (LCL Port: Luiz AmÃ©rico)"/>
    <CompilerOptions>
      <Version Value="11"/>
      <SearchPaths>
        <IncludeFiles Value="../../Source/include/intf/$(LCLWidgetType);../../Source/units;../../Source/include/intf"/>
